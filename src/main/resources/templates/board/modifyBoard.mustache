<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Insert title here</title>
<!-- Bootstrap CSS -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
</head>
<body>
{{>layouts/header}}

{{#msg}}
	<div>
		{{msg}}
	</div>
{{/msg}}
<table class="table">
    <thead>
    <tr>
        <th scope="col">BoardNO</th>
        <th scope="col">BoardTitle</th>
        <th scope="col">BoardContent</th>
    </tr>
    </thead>
    <tbody>
    {{#article}}
    <tr>
        <th>{{boardNo}}</th>
        <td>{{boardTitle}}</td>
        <td>{{boardContent}}</td>
    </tr>
    {{/article}}
    </tbody>
</table>

<a href="/board/addBoard?boardNo={{board.boardNo}}" class="btn btn-primary">Edit</a>
<!--<a href="/articles/delete?id={{article.id}}" class="btn btn-danger">Delete</a>-->
<a href="/board">Go to Board List</a>


{{>layouts/footer}}
</body>
</html>